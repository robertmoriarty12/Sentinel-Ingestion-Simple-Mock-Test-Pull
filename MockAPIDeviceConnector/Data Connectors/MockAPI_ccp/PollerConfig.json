[
  {
    "name": "MockAPIPoller",
    "apiVersion": "2022-10-01-preview",
    "type": "Microsoft.SecurityInsights/dataConnectors",
    "kind": "RestApiPoller",
    "properties": {
      "connectorDefinitionName": "MockAPIDeviceConnector",
      "dcrConfig": {
        "streamName": "Custom-MockAPIDevicesStream",
        "dataCollectionEndpoint": "[[parameters('dcrConfig').dataCollectionEndpoint]",
        "dataCollectionRuleImmutableId": "[[parameters('dcrConfig').dataCollectionRuleImmutableId]"
      },
      "dataType": "MockAPIDevices_CL",
      "auth": {
        "type": "APIKey",
        "ApiKey": "[[parameters('apiKey')]",
        "ApiKeyName": "X-API-Key"
      },
      "request": {
        "apiEndpoint": "[[parameters('apiEndpoint')]",
        "httpMethod": "GET",
        "rateLimitQPS": 1,
        "queryWindowInMin": 5,
        "retryCount": 3,
        "timeoutInSeconds": 30,
        "headers": {
          "Accept": "application/json",
          "User-Agent": "Sentinel-MockAPI-Connector"
        }
      },
      "response": {
        "eventsJsonPaths": [
          "$.data"
        ]
      }
    }
  }
]
